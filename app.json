{
  "expo": {
    "newArchEnabled": true,
    "name": "retashot",
    "slug": "retashot",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "scheme": "retashot",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-video",
      "@react-native-google-signin/google-signin"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.retashot.app",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.retashot.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "navigationBarColor": "transparent",
      "navigationBarHidden": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "retashot",
              "pathPrefix": "/login"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.googleusercontent.apps.650817826399-u3o28atpe9sr83p94c391bnup59eoksm",
              "pathPrefix": "/oauth2redirect"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70472758-868e-4e6a-bf4c-87a24b22d26b"
      }
    },
    "owner": "dawszz_4"
  }
}
